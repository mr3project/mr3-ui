{{#if loaded}}
  {{#if model.entityID}}
    <table class='detail-list'>
      <thead>
      <tr>
        <th colspan=2>Details</th>
      </tr>
      </thead>
      <tbody>
      <tr>
        <td>ID</td>
        <td>{{model.containerID}}</td>
      </tr>
      <tr>
        <td>End Time</td>
        <td>{{model.endTime}}</td>
      </tr>
      </tbody>
    </table>
  {{/if}}

  {{#unless model.entityID}}
    <h2>Some data is not available!</h2>
    <h5>No data returned from URL:
      <i>
        {{#unless model.entityID}}
          {{hosts.timeline}}/{{env.app.namespaces.webService.timeline}}/{{env.app.paths.timeline.container}}/{{model.entityID}}
        {{/unless}}
      </i>.
      The data may not be available in Timeline server (ATS) or you may not have the necessary permissions to view this data.
    </h5>
  {{/unless}}

{{else}}
  {{partial "loading"}}
{{/if}}
